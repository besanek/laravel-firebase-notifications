{
    "name": "besanek/laravel-firebase-notifications",
    "description": "Thin layer for integrate Firebase Messaging as Laravel Notifications",
    "keywords": ["filesystem", "laravel", "notifications", "firebase"],
    "type": "library",
    "require": {
        "illuminate/notifications": "^8.0",
        "illuminate/support": "^8.0",
        "php": "^7.4|^8.0",
        "kreait/laravel-firebase": "^3.1",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4.2"
    },
    "authors": [
        {
            "name": "Robert Jelen",
            "email": "robert@jelen.dev"
        }
    ],
    "autoload": {
        "psr-4": {
            "Besanek\\LaravelFirebaseNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Besanek\\LaravelFirebaseNotifications\\FirebaseServiceProvider"
            ]
        }
    }
}
